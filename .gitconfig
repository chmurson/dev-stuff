[alias]
	st = status
	c = checkout
	ph = push
	pl = pull
	phf = push --force
	s = status -s
        cm = commit -m
        logg = log --graph --decorate --oneline 
        loggpr = log --graph --decorate --oneline --grep="Merge pull request"
        o = open
        r = recent 
